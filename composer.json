{
    "name": "avonture/pandoc",
    "description": "PHP class that encapsulates pandoc in order to convert a text in markdown format to doc/odt/pdf/txt/...",
    "homepage": "https://github.com/cavo789/pandoc",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Christophe Avonture",
            "email": "christophe@avonture.be"
        }
    ],
    "keywords": [
        "php",
        "pandoc",
        "convert",
        "markdown",
        "md2docx",
        "md2pdf",
        "md2txt",
        "md2odt"
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.2.0"
    },
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^8.2"
    }
}
